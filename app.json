{
  "expo": {
    "name": "Physics with Mr. Saddam",
    "slug": "physics-mr-saddam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#001F3F"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bedaya.physics"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#001F3F"
      },
      "package": "com.bedaya.physics"
    },
    "web": {
      "favicon": "./assets/icon.png",
      "bundler": "webpack"
    },
    "platforms": ["ios", "android", "web"],
    "scheme": "physics-mr-saddam"
  }
}